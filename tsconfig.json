{
	"compilerOptions": {
			"declaration": true,
			"module": "commonjs",
			"target": "es5",
			"noImplicitAny": false,
			"sourceMap": true,
			"noEmit": false,
			"outDir": "out",
			"rootDir": "src",
			"lib": [
				"es2017",
				"dom"
			]
		},
		"include": [
			"./src/**/*.ts"
		],
		"exclude": [
			"node_modules"
		]
}
